{
  "name": "scrabble",
  "version": "0.0.1",
  "description": "A Scrabble game.",
  "author": "Jon Rubins <jonrubins@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "compile": "yarn --production=false && tsc",
    "lint": "yarn --production=false && yarn lint:js",
    "lint:js": "eslint src babel.config.js buildConfig.js postcss.config.js prettier.config.js webpack.config.dev.js webpack.config.prod.js --cache --ext=.js,.jsx,.ts,.tsx",
    "lint:sass": "sass-lint -v -q",
    "start": "yarn && APP_ENV=development NODE_ENV=development webpack-dev-server --progress --config webpack.config.dev.js"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6",
    "yarn": "1.22.4"
  },
  "browserslist": "> 1%",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache --ext=.js,.jsx,.ts,.tsx"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@xstate/react": "0.8.1",
    "classnames": "2.2.6",
    "lodash": "4.17.20",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "uuid": "8.3.0",
    "xstate": "4.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/types": "7.11.5",
    "@hot-loader/react-dom": "16.13.0",
    "@types/classnames": "2.2.10",
    "@types/lodash": "4.14.161",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "copy-webpack-plugin": "6.1.0",
    "core-js": "3.6.5",
    "css-loader": "3.6.0",
    "eslint": "7.8.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "eslint-plugin-require-path-exists": "1.1.9",
    "file-loader": "6.1.0",
    "history": "4.10.1",
    "html-webpack-plugin": "4.4.1",
    "husky": "4.2.5",
    "lint-staged": "10.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.1.1",
    "react-hot-loader": "4.12.21",
    "react-test-renderer": "16.13.1",
    "sass-lint": "1.13.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "typescript": "3.9.6",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
